   "source": [
    "fig, ax = plt.subplots(1,3, figsize=(15, 7))\n",
    "fig.tight_layout()\n",
    "df_platform = df_actual.pivot_table(index='platform', \n",
    "                      values=['na_sales', 'eu_sales', 'jp_sales'],\n",
    "                      aggfunc='sum').reset_index()\n",
    "labels_na = list(df_platform.sort_values(by='na_sales', ascending=False).head(5)['platform'])\n",
    "labels_eu = list(df_platform.sort_values(by='eu_sales', ascending=False).head(5)['platform'])\n",
    "labels_jp = list(df_platform.sort_values(by='jp_sales', ascending=False).head(5)['platform'])\n",
    "ax[0].pie(df_platform.sort_values(by='na_sales', ascending=False).head(5)['na_sales'], \n",
    "          labels=labels_na,\n",
    "          autopct='%1.1f%%')\n",
    "ax[0].set_title('Северная Америка')\n",
    "ax[1].pie(df_platform.sort_values(by='eu_sales', ascending=False).head(5)['eu_sales'], \n",
    "          labels=labels_eu,\n",
    "          autopct='%1.1f%%')\n",
    "ax[1].set_title('Европа')\n",
    "ax[2].pie(df_platform.sort_values(by='jp_sales', ascending=False).head(5)['jp_sales'], \n",
    "          labels=labels_jp,\n",
    "          autopct='%1.1f%%')\n",
    "ax[2].set_title('Япония')\n",
    "plt.show()"
   ]
